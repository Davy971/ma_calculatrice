        android:id="@+id/button53"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:onClick="sin_G"
        android:text="sin(x)"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="8sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/guideline_9"
        app:layout_constraintRight_toLeftOf="@+id/guideline_10"
        app:layout_constraintTop_toBottomOf="@+id/guideline_7" />

    <Button
        android:id="@+id/button54"
        style="@android:style/Widget.DeviceDefault.Button.Inset"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:onClick="btnEgal_G"
