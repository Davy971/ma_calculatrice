        android:text="@string/mult"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline_4"
        app:layout_constraintLeft_toRightOf="@+id/guideline_9"
        app:layout_constraintRight_toLeftOf="@+id/guideline_10"
        app:layout_constraintTop_toBottomOf="@+id/guideline_3" />

    <Button
