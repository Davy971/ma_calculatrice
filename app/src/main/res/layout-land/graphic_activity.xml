        android:layout_height="0dp"
        android:onClick="btn_ajout_caract_G"
        android:text="@string/_2"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline_5"
        app:layout_constraintLeft_toRightOf="@+id/guideline_8"
        app:layout_constraintRight_toLeftOf="@+id/guideline_9"
        app:layout_constraintTop_toTopOf="@+id/guideline_4" />

    <Button
        android:id="@+id/button33"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:onClick="btn_ajout_caract_G"
        android:text="@string/_3"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline_5"
        app:layout_constraintLeft_toRightOf="@+id/guideline_9"
        app:layout_constraintRight_toLeftOf="@+id/guideline_10"
        app:layout_constraintTop_toTopOf="@+id/guideline_4" />

    <Button
        android:id="@+id/button34"
        style="@android:style/Widget.DeviceDefault.Button.Inset"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:onClick="btn_ajout_caract_G"
        android:text="@string/add"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline_5"
        app:layout_constraintLeft_toRightOf="@+id/guideline_10"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline_4" />

    <Button
        android:id="@+id/button35"
